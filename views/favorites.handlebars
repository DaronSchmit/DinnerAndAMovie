<div class="container content">

  <h1>This is all your history section</h1>

  <br>

  <div class="row">
    <div class="col-lg-12 bg-warning text-center">
      <h3>Previous Movies</h3>

      <button class=" submit delete-favorite ">Delete</button>

    </div>
  </div>

  <div class="container m-3 p-3">
    <div class="row">
      <div class="col-md-6">
        <h2> Favorite Movies</h2>
        <div class="=content-dinners">
          {{!-- <ul> --}}
          {{#each dinner}}
          {{#unless favorited}}
          {{> dinner/history-block favorited=true}}
          {{/unless}}
          {{/each}}
        </div>
        {{!-- </ul> --}}
      </div>

      <div class="col-md-6">
        <h2>Favorites Dinner and Recipes</h2>

        {{!-- <ul> --}}
        {{#each dinner}}
        {{#if favorited}}
        {{> dinner/history-block favorited=false}}
        {{/if}}
        {{/each}}
        {{!-- </ul> --}}
        <a class="btn btn-light" href="/">New Post</a>
        <a class="btn btn-light" href="/homepage">New Post</a>

      </div>
    </div>
  </div>
</div>
</div>